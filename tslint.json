{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "plugins": ["prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "import-name": false,
    "no-any": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "prettier": true
  }
}
